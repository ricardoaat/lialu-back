{
  "name": "NodeAPI",
  "version": "1.0.0",
  "author": "Ricardo Arcila - Pensive Borg",
  "description": "Lialu NodeJS Backend",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "dbdocker-destroy": "docker stop redis_borg mongo_borg && docker rm redis_borg mongo_borg",
    "dbdocker-build": "docker run --name redis_borg -p 6379:6379 -d redis && docker run --name mongo_borg -p 27017:27017 -d mongo --auth",
    "db-up": "docker start mongo_borg && docker start redis_borg",
    "dstart": "nodemon --ignore 'public/*' /bin/www"
  },
  "dependencies": {
    "acl": "^0.4.10",
    "bluebird": "^3.4.7",
    "body-parser": "~1.15.x",
    "cookie-parser": "~1.4.x",
    "debug": "~2.6.x",
    "express": "~4.14.x",
    "express-jwt": "^5.1.0",
    "faker": "^3.1.x",
    "jsonwebtoken": "^7.1.9",
    "lodash": "^4.17.2",
    "method-override": "^2.3.x",
    "mongoose": "^4.6.x",
    "morgan": "^1.7.0",
    "nconf": "^0.8.x",
    "redis": "^2.6.4",
    "winston": "^2.3.x"
  },
  "jshintConfig": {
    "curly": true,
    "eqeqeq": true,
    "immed": true,
    "latedef": false,
    "newcap": false,
    "noarg": true,
    "sub": true,
    "undef": true,
    "boss": true,
    "eqnull": true,
    "strict": false,
    "scripturl": true,
    "evil": true,
    "globals": {
      "location": true,
      "printStackTrace": false
    },
    "browser": true,
    "node": true
  },
  "license": "MIT",
  "devDependencies": {
    "jshint": "^2.9.4"
  }
}
