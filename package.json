{
  "name": "NodeAPI",
  "version": "1.0.0",
  "author": "Ricardo Arcila",
  "description": "Lialu NodeJS Backend",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "dbdocker-build": "docker run --name lialu -p 27017 -d mongo",
    "db-up": "docker start lialu",
    "devstart": "nodemon /bin/www"
  },
  "dependencies": {
    "body-parser": "~1.15.x",
    "cookie-parser": "~1.3.x",
    "debug": "~2.0.x",
    "express": "~4.14.x",
    "express-jwt": "^5.1.0",
    "faker": "^2.0.x",
    "jsonwebtoken": "^7.1.9",
    "lodash": "^4.17.2",
    "method-override": "^2.3.x",
    "mongoose": "^4.6.x",
    "morgan": "^1.7.0",
    "nconf": "^0.8.x",
    "oauth2orize": "^1.0.x",
    "passport": "^0.2.x",
    "passport-http": "^0.2.x",
    "passport-http-bearer": "^1.0.x",
    "passport-oauth2-client-password": "^0.1.x",
    "winston": "^2.3.x"
  },
  "jshintConfig": {
    "curly": true,
    "eqeqeq": true,
    "immed": true,
    "latedef": false,
    "newcap": false,
    "noarg": true,
    "sub": true,
    "undef": true,
    "boss": true,
    "eqnull": true,
    "strict": false,
    "scripturl": true,
    "evil": true,
    "globals": {
      "location": true,
      "printStackTrace": false
    },
    "browser": true,
    "node": true
  },
  "license": "MIT",
  "devDependencies": {
    "jshint": "^2.9.4"
  }
}
